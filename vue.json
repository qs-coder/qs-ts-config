{
  "display": "Vue Application",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "bundler",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "declaration": false,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true
  }
}
